package com.cts.ofds.model;

public class Cart {
	private int cartId;
	private String userId;
	private Dish dish;
	
	public Cart() {	}
	
	public Cart(String userId, Dish dish) {
		this.userId = userId;
		this.dish = dish;
	}
	public int getCartId() {
		return cartId;
	}
	public void setCartId(int cartId) {
		this.cartId = cartId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public Dish getDish() {
		return dish;
	}
	public void setDish(Dish dish) {
		this.dish = dish;
	}

	@Override
	public String toString() {
		return "Cart [cartId=" + cartId + ", userId=" + userId + ", dish=" + dish + "]";
	}
	
}
